                                        <select class="form-control" id="IDKhoaHoc" >
                                              </select>
                                </div>
                            </div>
                        </div>
                          <div class="row">
                            <div class="col">
                                <div class="form-group">
                                    <label class="control-label">Số CMND_CCCD</label>
                                    <input id="soCMND_CCCD" name="soCMND_CCCD" class="form-control" type="text" />
