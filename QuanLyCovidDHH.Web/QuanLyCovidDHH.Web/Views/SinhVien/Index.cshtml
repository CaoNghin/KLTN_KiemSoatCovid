@section scripts{

      

    <script>


        $(document).ready(function () {
              
            Load_Danhsach_SinhVien();
            Load_DSTruong_Combobox();
            Load_DSKhoa_Combobox();
            ValidateForm();
            $('#trangThai').hide();
            //$('#ngayBanHanh').datepicker();


        });
        function ResetForm() {
            
            $('#soCMND_CCCD').val('');
             $('#hoTen').val('');
            $('#diaChi').val('');
           $('#sDT').val('');
            $('#email').val('');

          

        };
         function ThongBao(messenger) {
            $("#notiMessenger").empty();
            $("#notiMessenger").append(messenger);
            $("#btnConfirm").hide();
            $("#notification").modal();
        };
        function ValidateForm() {
            $("form[id='formSinhVien']").validate({
                rules: {
                    maKhoa: {
                        required: true,
                        minlength: 1,
                        maxlength: 50
                    },
                    tenKhoa: {
                        required: true,
                        minlength: 1,
                        maxlength: 50
                    },
                  
                   
                },
                messages: {
                    maKhoa: {
                        required: "Vui lòng nhập Mã khoa",
                        minlength: "Chiều dài phải lớn hơn 1 ký tự",
                        maxlength: "Chiều dài không vượt quá 50 kí tự"
                    },
                    tenKhoa: {
                        required: "Vui lòng nhập Tên khoa",
                        minlength: "Chiều dài phải lớn hơn 1 ký tự",
                        maxlength: "Chiều dài không vượt quá 50 ký tự"
                    },
                
                    
                },
                errorElement: 'div',
                errorLabelContainer: '.errorTxt',
                submitHandler: function (form) {
                }
            });
        }
        $('#btnAdd').click(function () {
            ResetForm();
         
      
          $('#hiddenvalue').val(''); // Hidden id
            $('#IDTruongAll').show();
            $('#IDKhoaAll').show();
            $('#IDKhoaHocAll').show();
            $('#modalSinhVien').modal();
            $('#btnKhaiBaoXetNghiem').hide();
            $('#btnKhaiBaoCachLy').hide();
            $('#btnChiTietTiem').hide();
            $('#btnBenhAn').hide();

        });
        function Load_Danhsach_SinhVien() {
            TaoBang();  
    
            $.ajax({
                url: 'SinhVien/DanhSach',
                type: 'GET',
                data: {
                     IDKhoaHoc: null,
                     IDKhoa: null,
                    IDTruong: null,
                    TuKhoa:null
             

                },
                success: function (data) {
                    if (data.code == 200) {
                        let stt = 0;

                        $.each(data.sinhvien, function (key, value) {

                             stt = stt + 1;

                            let tr = '<tr id="' + value.idSinhVien+ '">';
                            tr += '<td>' + stt + '</td>';  
                            tr += '<td>' + value.hoTen + '</td>';
                            tr += '<td>' + value.tenDangNhap + '</td>';
                            tr += '<td>' + value.tongMuiTiem + '</td>';
                            tr += '<td>' + value.daMacCovid + '</td>';
                            tr += '<td>' + value.khoaHoc + '</td>';
                            tr += '<td>' + value.tenKhoa + '</td>';
                            tr += '<td>' + value.tenTruong + '</td>';
                            tr += '<td>'
                            tr += '<button class="btn btn-sm" name="edit" style="color:green"><i class="fa fa-info-circle"></i></button>';
                            tr += '<button class="btn btn-sm" name="delete" style="color:red"><i class="fa fa-trash"></i></button>';
                            tr += '</td>'
                            tr += '</tr>'
                            $('#table-body').append(tr);

                            //////////
                           

                    /////
                        });
                   Load_PhanTrang();
                    }
                }
            });
        };

        $('#btnSearch').click(function () {
                //Xoa_PhanTrang();      
            let select1 = document.getElementById('timkiem-timtheoIDTruong');
            let option1 = select1.options[select1.selectedIndex];
            let idtruong = option1.value;

            let select2 = document.getElementById('timkiem-timtheoIDKhoa');
            let option2 = select2.options[select2.selectedIndex];
            let idkhoa = option2.value;

             let select3 = document.getElementById('timkiem-timtheoIDKhoaHoc');
            let option3 = select3.options[select3.selectedIndex];
            let idkhoahoc = option3.value;
            let tukhoa = $('#timkiem-timtheotukhoa').val();
            //alert(idtruong+idkhoa+idkhoahoc);
            $.ajax({
                url: 'SinhVien/DanhSach',
                type: 'GET',
                data: {
                    IDKhoaHoc:idkhoahoc,
                    IDKhoa: idkhoa,
                    IDTruong: idtruong,
                    TuKhoa: tukhoa,
              
                },
                success: function (data) {
               if (data.code == 200) {
                        let stt = 0;
              
                        $('#table-full').empty();
                       
                        TaoBang();
                         
                        $.each(data.sinhvien, function (key, value) {
                             stt = stt + 1;
                            //Convert Datetime to String
                            @*let today = new Date(value.ngayBanHanh);
                            let dd = String(today.getDate()).padStart(2, '0');
                            let mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
                            let yyyy = today.getFullYear();
                            today = dd + '/' + mm + '/' + yyyy;*@

                                let tr = '<tr id="' + value.idSinhVien+ '">';
                            tr += '<td>' + stt + '</td>';  
                            tr += '<td>' + value.hoTen + '</td>';
                            tr += '<td>' + value.tenDangNhap + '</td>';
                            tr += '<td>' + value.tongMuiTiem + '</td>';
                            tr += '<td>' + value.daMacCovid + '</td>';
                            tr += '<td>' + value.khoaHoc + '</td>';
                            tr += '<td>' + value.tenKhoa + '</td>';
                            tr += '<td>' + value.tenTruong + '</td>';
                            tr += '<td>'
                            tr += '<button class="btn btn-sm" name="edit" style="color:green"><i class="fa fa-info-circle"></i></button>';
                            tr += '<button class="btn btn-sm" name="delete" style="color:red"><i class="fa fa-trash"></i></button>';
                            tr += '</td>'
                            tr += '</tr>'
                            $('#table-body').append(tr);
                        });
                        destroy();
                   //Load_PhanTrang();
                       
                    }
                }
            });
        });

       function ThongBao(messenger) {
            $("#notiMessenger").empty();
            $("#notiMessenger").append(messenger);
            $("#btnConfirm").hide();
            $("#notification").modal();
        };

        $('#btnSubmit').click(function () {
            let hiddenvalue = $('#hiddenvalue').val();
            let IDKhoaHoc = $('#IDKhoaHoc').val().trim();
            let SoCMND_CCCD = $('#soCMND_CCCD').val().trim();
            let HoTen = $('#hoTen').val().trim();
            let DiaChi = $('#diaChi').val().trim();
            let SDT = $('#sDT').val().trim();
            let Email = $('#email').val().trim();



            //let trangThai = $('input[id="trangThai"]:checked').val();

            let formData1 = new FormData();


            formData1.append('IDKhoaHoc', IDKhoaHoc);
            formData1.append('SoCMND_CCCD', SoCMND_CCCD);
            formData1.append('HoTen', HoTen);
            formData1.append('DiaChi', DiaChi);
            formData1.append('SDT', SDT);
            formData1.append('Email', Email);
           

            let MatKhau = $('#matKhau').val().trim();


            let formData2 = new FormData();
            formData2 = formData1;
            formData2.append('IDSinhVien', hiddenvalue);

            formData2.append('MatKhau', MatKhau);
         
         
            //formData2.append('trangThai', trangThai);
           
          

            if ($("#formSinhVien").valid()) {
                if (hiddenvalue.length == 0) {
                   
                    //Thêm mới
                    $.ajax({
                        url: '/SinhVien/TaoMoi',
                        type: 'POST',
                        data: formData1,
                        contentType: false,
                        processData: false,
                        success: function (data) {
                            if (data.code == 200) {
                                console.log(formData1);
                                ResetForm();
                                $('#btnClose').click();
                                ThongBao(data.msg);
                                $('#table-body').empty();
                                 $('#btnSearch').click();
                            } else {
                                      

                                $('#btnClose').click();
                                ThongBao(data.msg);
                            }
                        }
                    });
                }
                else if (hiddenvalue.length != 0) {
                    //Chỉnh sửa

                    $.ajax({
                        url: '/SinhVien/ChinhSua',
                        type: 'PUT',
                        data: formData2,
                        contentType: false,
                        processData: false,
                        success: function (data) {
                            if (data.code == 200) {
                                $('#table-body').empty();
                                 $('#btnSearch').click();
                                $('#btnClose').click();
                                ThongBao(data.msg);
                            } else {
                                $('#btnClose').click();
                                ThongBao(data.msg);
                            }
                        }
                    })
                }
            }
        });

         $(document).on('click', "button[name='edit']", function () {
            ResetForm();

          
            let id = $(this).closest('tr').attr('id');
            $('#hiddenvalue').val(id);
                 

                   
             
             
            $.ajax({
                url: '/SinhVien/LayTheoID',
                type: 'GET',
                data: { id: id },
                success: function (data) {
              
                //$("#IDTruong").val(data.sinhvien.iDTruong).change();
                //    alert(data.sinhvien.iDTruong);
                //let IDTruong = data.sinhvien.idTruong;
             
                //let IDKhoa=data.sinhvien.idKhoa;
      

                //  Load_Khoa_Combobox_ChinhSua(IDTruong,IDKhoa);
                //     TAI DU LIEU KHOA COMBOBOX
              
                  
                //      $('#trangThai').show();
                //    $('#IDKhoaHoc').val(data.sinhvien.iDKhoaHoc);

                    $('#btnKhaiBaoXetNghiem').show();
                    $('#btnKhaiBaoCachLy').show();
                    $('#btnChiTietTiem').show();
                    $('#btnBenhAn').show();

                    $('#btnKhaiBaoXetNghiem').attr('href', 'KhaiBaoXetNghiem?IDSinhVien='+id);
                    $('#btnKhaiBaoCachLy').attr('href', 'KhaiBaoCachLy?IDSinhVien='+id);
                    $('#btnChiTietTiem').attr('href', 'ChiTietTiem?IDSinhVien='+id);
                    $('#btnBenhAn').attr('href', 'BenhAn?IDSinhVien='+id);
                ///////////////////////////////////////////

                    $('#IDTruongAll').hide();
                    $('#IDKhoaAll').hide();
                    $('#IDKhoaHocAll').hide();
               
                
                                $('#soCMND_CCCD').val(data.sinhvien.soCMND_CCCD);
               
                                $('#hoTen').val(data.sinhvien.hoTen);
         
                                $('#diaChi').val(data.sinhvien.diaChi);
                
                                $('#sDT').val(data.sinhvien.sdt);
            
                   
                                $('#email').val(data.sinhvien.email);
                  
                    
                    $('#tongMuiTiem').val(data.sinhvien.tongMuiTiem);
                    $('#daMacCovid').val(data.sinhvien.daMacCovid);
                    $('#tenDangNhap').val(data.sinhvien.tenDangNhap);
                    //$('#matKhau').val(data.sinhvien.matKhau);
                    //alert(data.sinhvien.DiaChi +";"+data.sinhvien.diaChi +";"+data.sinhvien.sdt +";"+data.sinhvien.matKhau);
         
                      $('#matKhau').val(String(data.sinhvien.matKhau));
                 

                     
                 
                    $('#modalSinhVien').modal();
                }
            });
  
         });
        $(document).on('click', "button[name='delete']", function () {
            let id = $(this).closest('tr').attr('id');
            var noti = confirm("Bạn có muốn xóa không?");
            if (noti == true) {
                $.ajax({
                    url: 'SinhVien/XoaBo',
                    type: 'DELETE',
                    data: { id: id },
                    success: function (data) {
                        if (data.code == 200) {
                            $('#table-body').empty();
                            $('#btnSearch').click();
                            ThongBao(data.msg);
                        }
                        else ThongBao(data.msg);
                    }
                })
            } else {
                alert("You pressed Cancel!");
            }
        });
 
        function Load_DSTruong_Combobox() {
            $.ajax({
                url: 'Truong/DanhSach',
                type: 'GET',
                data: {
             
                },
                success: function (data) {
                    if (data.code == 200) {
            
                        $.each(data.truong, function (key, value) {
                            let option = '<option  value="' + value.idTruong + '" class="form-control">' +  value.tenTruong+ '</option>';
                            $('#timkiem-timtheoIDTruong').append(option);
                            $('#IDTruong').append(option);
                        });
                    }
                }
            });
        };
             function Load_DSKhoa_Combobox() {
            $.ajax({
                url: 'Khoa/DanhSach',
                type: 'GET',
                data: {
             
                },
                success: function (data) {
                    if (data.code == 200) {
            
                        $.each(data.khoa, function (key, value) {
                            let option = '<option  value="' + value.idKhoa + '" class="form-control">' +  value.tenKhoa+ '</option>';
                            $('#timkiem-timtheoIDKhoa').append(option);
                            $('#IDKhoa').append(option);
                        });
                    }
                }
            });
        };
        function TruongChanged(obj) {
            var message = document.getElementById('timkiem-timtheoIDTruong');
            var value = obj.value;
            $("#timkiem-timtheoIDKhoa").empty();
           let option = ' <option class="form-control" value="">- Tất cả -</option>';
                            $('#timkiem-timtheoIDKhoa').append(option);

                                    
             //alert("You selected:"+value);
              $.ajax({
                url: 'Khoa/DanhSach',
                type: 'GET',
                data: {
                    IDTruong:value
             
