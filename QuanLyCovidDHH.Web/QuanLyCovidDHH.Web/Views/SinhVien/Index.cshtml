                        <span>Thêm mới</span>
                    </a>
                
                    <a class="btn btn-primary" id="btnSearch">
                        <i class="fa fa-search"></i>
                        <span>Tìm kiếm</span>
                    </a>
                              
                    
                </div>
            </form>
        </div>
      
        <div class="box-bordertop"> 
        
       
            <table class="table table-hover table-bordered" id="table-full">
         
         
            </table>
                  
                  <button class="btn btn-success" onclick="exportTableToExcel('table-full', 'sinhvien-data')">Xuất File Excel</button>
                 
        </div>
    </div>



